div(class="row")
  div(class="col-md-10 col-md-offset-1")
    div(class="jumbotron")
      h1 {{ SpotifyLookup.results.name }}

div(class="row")
  div(class="col-md-10 col-md-offset-1")
    div(class="row")
      div(ng-repeat="a in SpotifyLookup.results.albumsUS", class="col-md-6 search-result")
        h4(class="album") 
          a(ng-href="/album/{{ a.album.href }}") {{ a.album.name }}
        p(class="artist") Artist: {{ a.album.artist }}
        p(class="year") Year: {{ a.album.released }}
